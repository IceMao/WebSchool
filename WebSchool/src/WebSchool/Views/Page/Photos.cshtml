@{
    Layout = "~/Views/Shared/_Layout1.cshtml";
}
@model IEnumerable<Photos>
<div class="col-md-9">
    <div class="container-fluid" id="accordion">
        <div class="panel-group" id="accordion">
            @foreach (var x in Model)
            {
                <div class="panel panel-default ">
                    <div class="panel-heading">
                        <h4 class="panel-title">
                            <a data-toggle="collapse" data-parent="#accordion"
                               href="#@x.Id" style="color:#000000;text-decoration:none;">
                                @x.Title
                            </a>
                        </h4>
                    </div>
                    <div id="@x.Id" class="panel-collapse collapse">
                        <div class="panel-body">
                            <p class="text-muted">
                                <small>发布时间:2015-12-07</small>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<small>文章作者:Cream</small>
                                <small class="pull-right">点击量：<span class="badge">99+</span></small>
                            </p>
                            <pre>@x.Discription</pre>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
    <div class="col-sm-6 col-md-6"  style="margin:0 auto;">
        <div class="thumbnail">
            <img src="~/images/department.jpg"
                 alt="图片加载失败哟~">
        </div>
        <div class="caption">
            <p>校园一角</p>
        </div>
    </div>
    <div class="col-sm-6 col-md-6" style="margin:0 auto;">
        <div class="thumbnail">
