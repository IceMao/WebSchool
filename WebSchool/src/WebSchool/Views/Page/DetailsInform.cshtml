@{
    Layout = "~/Views/Shared/_Layout1.cshtml";
}
@model IEnumerable<Inform>
<table class="table">
    <thead>
        <tr>
            <th>通知ID</th>
            <th>通知标题</th>
            <th>录入时间</th>
            <th>通知内容</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model)
        {
            <tr>
                <th>@x.Id</th>
                <th>@x.Title</th>
                <th>@x.Datatime</th>
                <th>@x.Content</th>
                
            </tr>
            
        }
    </tbody>
    
</table>
@foreach (var x in Model)
{
<div class="panel-body">
    <strong class="panel-title"><a href="#" style="text-decoration:none;">@x.Title</a></strong>
    <p>@x.Content</p>
</div>
}